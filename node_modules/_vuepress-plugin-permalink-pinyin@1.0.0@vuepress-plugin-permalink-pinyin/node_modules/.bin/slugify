#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_transliteration@2.2.0@transliteration/dist/bin/slugify" "$@"
  ret=$?
else 
  node  "$basedir/../../../_transliteration@2.2.0@transliteration/dist/bin/slugify" "$@"
  ret=$?
fi
exit $ret
